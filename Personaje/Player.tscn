script/source = "# Agregar estas variables al inicio de tu script del jugador
extends CharacterBody2D

const SPEED = 20.0
const JUMP_VELOCITY = -60.0
const GRAVITY = 300.0
const FALL_MULTIPLIER = 0.5
const LOW_JUMP_MULTIPLIER = 0.8

# NUEVO: L√≠mite de ca√≠da para c√°mara
const LIMITE_CAIDA_Y = 20.0  # Cuando Y > 20, fijar c√°mara

# NUEVO: Posici√≥n donde se fija la c√°mara al caer
var posicion_fija_camara := Vector2.ZERO  # Se configura autom√°ticamente

# NUEVO: Variable para control de c√°mara en saltos
var camara_siguiendo := true

var tiene_armadura := false
var esta_muerto := false

# NUEVO: Variable para control de ca√≠da
var ya_inicio_muerte_caida := false

# VARIABLES DE AUDIO CON VERIFICACI√ìN DE SEGURIDAD
@onready var sonido_pasos = get_node_or_null(\"SonidoPasos\")
@onready var sonido_salto = get_node_or_null(\"SonidoSalto\")
@onready var sonido_muerte = get_node_or_null(\"SonidoMuerte\")
@onready var sonido_da√±o = get_node_or_null(\"SonidoDa√±o\")
@onready var sonido_disparo = get_node_or_null(\"SonidoDisparo\")
@onready var sonido_curar_vida = get_node_or_null(\"SonidoCurarVida\")
@onready var sonido_armadura = get_node_or_null(\"SonidoArmadura\")

# Sistema de vida
var vida := 125
var vida_maxima := 125

# SISTEMA DE DISPARO MEJORADO
var puede_disparar := true
var tiempo_recarga := 0.2
var esta_disparando := false
var timer_recarga: Timer
@export var scene_bala: PackedScene

var barra_vida: TextureProgressBar = null

func _ready() -> void:
	# Verificar nodos de audio y reportar
	verificar_nodos_audio()
	
	barra_vida = get_node_or_null(\"BarraVida\")
	if barra_vida == null:
		print(\"No encontr√© la barra de vida\")
	else:
		barra_vida.max_value = vida_maxima
		barra_vida.value = vida
		actualizar_color_barra_vida()

	add_to_group(\"jugador\")
	
	# Crear timer de recarga
	timer_recarga = Timer.new()
	timer_recarga.wait_time = tiempo_recarga
	timer_recarga.one_shot = true
	timer_recarga.timeout.connect(_on_recarga_completa)
	add_child(timer_recarga)
	
	# Conectar se√±al de animaci√≥n
	if has_node(\"AnimatedSprite2D\"):
		$AnimatedSprite2D.animation_finished.connect(_on_animation_finished)
	
	# NUEVO: Conectar zonas de muerte si existen
	conectar_zonas_muerte()
	
	# NUEVO: Configurar posici√≥n fija por defecto (puedes cambiarlo)
	if posicion_fija_camara == Vector2.ZERO:
		# Por defecto, usar la posici√≥n inicial del jugador
		posicion_fija_camara = global_position + Vector2(0, -100)
		print(\"üì∑ Posici√≥n fija de c√°mara configurada en: \", posicion_fija_camara)

# NUEVA FUNCI√ìN: Conectar a zonas de muerte (opcional)
func conectar_zonas_muerte() -> void:
	# Buscar zonas de muerte en la escena
	var zonas_muerte = get_tree().get_nodes_in_group(\"zona_muerte\")
	for zona in zonas_muerte:
		if zona.has_signal(\"body_entered\"):
			zona.body_entered.connect(_on_zona_muerte_entered)
			print(\"‚úÖ Conectado a zona de muerte: \", zona.name)

# NUEVA FUNCI√ìN: Cuando entra a una zona de muerte (CORREGIDA)
func _on_zona_muerte_entered(body: Node) -> void:
	if body == self and not ya_inicio_muerte_caida:
		ya_inicio_muerte_caida = true
		print(\"üíÄ ZONA DE MUERTE: Entraste a una zona peligrosa\")
		fijar_camara_en_posicion()
		reproducir_sonido_seguro(sonido_muerte)

# NUEVA FUNCI√ìN: Verificar que todos los nodos de audio existan
func verificar_nodos_audio() -> void:
	print(\"=== VERIFICANDO NODOS DE AUDIO ===\")
	
	if sonido_pasos == null:
		print(\"‚ùå ERROR: SonidoPasos no encontrado\")
	else:
		print(\"‚úÖ SonidoPasos encontrado\")
	
	if sonido_salto == null:
		print(\"‚ùå ERROR: SonidoSalto no encontrado\")
	else:
		print(\"‚úÖ SonidoSalto encontrado\")
	
	if sonido_muerte == null:
		print(\"‚ùå ERROR: SonidoMuerte no encontrado\")
	else:
		print(\"‚úÖ SonidoMuerte encontrado\")
	
	if sonido_da√±o == null:
		print(\"‚ùå ERROR: SonidoDa√±o no encontrado\")
	else:
		print(\"‚úÖ SonidoDa√±o encontrado\")
	
	if sonido_disparo == null:
		print(\"‚ùå ERROR: SonidoDisparo no encontrado\")
	else:
		print(\"‚úÖ SonidoDisparo encontrado\")
	
	if sonido_curar_vida == null:
		print(\"‚ùå ERROR: SonidoCurarVida no encontrado\")
	else:
		print(\"‚úÖ SonidoCurarVida encontrado\")
	
	if sonido_armadura == null:
		print(\"‚ùå ERROR: SonidoArmadura no encontrado\")
	else:
		print(\"‚úÖ SonidoArmadura encontrado\")

func _physics_process(delta: float) -> void:
	if esta_muerto:
		return
	
	# NUEVO: Sistema de control de c√°mara por ca√≠da
	verificar_caida_para_camara(delta)
	
	# NUEVO: Sistema de control de c√°mara en saltos
	controlar_camara_en_saltos()
	
	# SISTEMA DE DISPARO MEJORADO
	if Input.is_action_just_pressed(\"disparar\"):
		if puede_disparar and not esta_disparando and is_on_floor():
			disparar()
	
	# Gravedad y salto
	if not is_on_floor():
		if velocity.y < 0:
			velocity.y += GRAVITY * LOW_JUMP_MULTIPLIER * delta
		else:
			velocity.y += GRAVITY * FALL_MULTIPLIER * delta
			
	if Input.is_action_just_pressed(\"saltar\") and is_on_floor() and not tiene_armadura:
		velocity.y = JUMP_VELOCITY
		# AUDIO SEGURO: Verificar antes de reproducir
		reproducir_sonido_seguro(sonido_salto)

	# Movimiento horizontal y animaciones
	var direction := Input.get_axis(\"mover_izquierda\", \"mover_derecha\")
	if direction > 0:
		$AnimatedSprite2D.flip_h = false
	elif direction < 0:
		$AnimatedSprite2D.flip_h = true

	if direction != 0:
		velocity.x = direction * SPEED
		# AUDIO SEGURO: Verificar antes de acceder a 'playing'
		if is_on_floor() and not esta_sonido_reproduciendose(sonido_pasos):
			reproducir_sonido_seguro(sonido_pasos)
	else:
		velocity.x = move_toward(velocity.x, 0, SPEED)
		# AUDIO SEGURO: Detener sonido de forma segura
		detener_sonido_seguro(sonido_pasos)

	# SISTEMA DE ANIMACIONES MEJORADO
	if not is_on_floor():
		if not esta_disparando:
			$AnimatedSprite2D.play(\"jump\")
	elif esta_disparando:
		# Usar animaci√≥n de disparo seg√∫n si tiene armadura o no
		var animacion_disparo = \"armadura_atacar\" if tiene_armadura else \"shot\"
		if $AnimatedSprite2D.animation != animacion_disparo:
			$AnimatedSprite2D.play(animacion_disparo)
	elif direction != 0:
		if tiene_armadura:
			$AnimatedSprite2D.play(\"armadura_caminar\")
		else:
			$AnimatedSprite2D.play(\"run\")
	else:
		if tiene_armadura:
			$AnimatedSprite2D.play(\"armadura_idle\")
		else:
			$AnimatedSprite2D.play(\"idle\")

	move_and_slide()

# NUEVA FUNCI√ìN: Control de c√°mara en saltos
func controlar_camara_en_saltos() -> void:
	if not has_node(\"Camera2D\"):
		return
	
	var camara = $Camera2D
	
	# Si est√° en el suelo y la c√°mara no est√° siguiendo, reactivar seguimiento
	if is_on_floor() and not camara_siguiendo:
		print(\"‚úÖ Jugador aterriz√≥ - Reactivando seguimiento de c√°mara\")
		camara.position_smoothing_enabled = true
		camara_siguiendo = true
	
	# Si est√° en el aire y la c√°mara est√° siguiendo, fijar c√°mara
	elif not is_on_floor() and camara_siguiendo:
		print(\"üöÅ Jugador salt√≥ - Fijando c√°mara\")
		camara.position_smoothing_enabled = false
		camara_siguiendo = false

# NUEVA FUNCI√ìN: Control de c√°mara por ca√≠da (CORREGIDA)
func verificar_caida_para_camara(delta: float) -> void:
	# Solo verificar l√≠mite de Y para fijar c√°mara
	if global_position.y > LIMITE_CAIDA_Y:
		if not ya_inicio_muerte_caida:
			ya_inicio_muerte_caida = true
			print(\"üíÄ CA√çDA DETECTADA: Jugador cay√≥ m√°s all√° de Y=\", LIMITE_CAIDA_Y)
			print(\"üì∑ Fijando c√°mara - El jugador sigue cayendo\")
			
			# CORREGIDO: Fijar la c√°mara en posici√≥n
			fijar_camara_en_posicion()
			
			# Reproducir sonido de muerte UNA sola vez
			reproducir_sonido_seguro(sonido_muerte)
			
			# El jugador contin√∫a cayendo normalmente
		# No hacer nada m√°s - el jugador sigue cayendo infinitamente

# NUEVA FUNCI√ìN: Fijar c√°mara en posici√≥n actual
func fijar_camara_en_posicion() -> void:
	if has_node(\"Camera2D\"):
		var camara = $Camera2D
		
		# Guardar la posici√≥n actual de la c√°mara
		posicion_fija_camara = camara.global_position
		
		# Desactivar seguimiento del jugador
		camara.position_smoothing_enabled = false
		
		# Hacer que la c√°mara se vuelva independiente
		hacer_camara_independiente()
		
		print(\"üì∑ C√ÅMARA FIJADA en posici√≥n: \", posicion_fija_camara)
	else:
		print(\"‚ö†Ô∏è No se encontr√≥ nodo Camera2D\")

# NUEVA FUNCI√ìN: Hacer la c√°mara independiente del jugador
func hacer_camara_independiente() -> void:
	if has_node(\"Camera2D\"):
		var camara = $Camera2D
		var posicion_actual = camara.global_position
		
		# Quitar la c√°mara del jugador
		remove_child(camara)
		
		# Agregar la c√°mara al nodo padre (nivel)
		get_parent().add_child(camara)
		
		# Fijar su posici√≥n
		camara.global_position = posicion_actual
		camara.position_smoothing_enabled = false
		
		print(\"üì∑ C√°mara ahora es independiente en: \", posicion_actual)

# NUEVAS FUNCIONES DE AUDIO SEGURO
func reproducir_sonido_seguro(nodo_audio: AudioStreamPlayer2D) -> void:
	if nodo_audio != null and nodo_audio.stream != null:
		nodo_audio.play()
	elif nodo_audio == null:
		print(\"‚ö†Ô∏è Intentando reproducir sonido null\")

func detener_sonido_seguro(nodo_audio: AudioStreamPlayer2D) -> void:
	if nodo_audio != null and esta_sonido_reproduciendose(nodo_audio):
		nodo_audio.stop()

func esta_sonido_reproduciendose(nodo_audio: AudioStreamPlayer2D) -> bool:
	if nodo_audio == null:
		return false
	return nodo_audio.playing

# FUNCI√ìN DE DISPARO MEJORADA
func disparar() -> void:
	print(\"=== INICIANDO DISPARO ===\")
	
	esta_disparando = true
	puede_disparar = false
	
	# Reproducir sonido de forma segura
	reproducir_sonido_seguro(sonido_disparo)
	
	# NUEVO: Usar animaci√≥n correcta seg√∫n armadura
	var animacion_disparo = \"armadura_atacar\" if tiene_armadura else \"shot\"
	$AnimatedSprite2D.play(animacion_disparo)
	print(\"Animaci√≥n '\", animacion_disparo, \"' iniciada\")
	
	# Crear la bala
	crear_bala()
	
	# Timer para finalizar disparo
	var timer_animacion = Timer.new()
	timer_animacion.wait_time = 0.8
	timer_animacion.one_shot = true
	timer_animacion.timeout.connect(_finalizar_disparo.bind(timer_animacion))
	add_child(timer_animacion)
	timer_animacion.start()

func crear_bala() -> void:
	if scene_bala != null:
		var bala = scene_bala.instantiate()
		get_parent().add_child(bala)
		
		# CORREGIDO: Volver a la posici√≥n que funcionaba
		bala.global_position = global_position + Vector2(0, -1.5)
		
		var direccion = 1 if not $AnimatedSprite2D.flip_h else -1
		
		if bala.has_method(\"set_direccion\"):
			bala.set_direccion(direccion)
		
		print(\"Bala creada en posici√≥n: \", bala.global_position)
	else:
		print(\"‚ùå ERROR: scene_bala es null\")

func _finalizar_disparo(timer_animacion: Timer) -> void:
	print(\"=== FINALIZANDO DISPARO ===\")
	
	esta_disparando = false
	
	if is_instance_valid(timer_animacion):
		timer_animacion.queue_free()
	
	if timer_recarga and not timer_recarga.is_stopped():
		timer_recarga.stop()
	
	timer_recarga.start()
	print(\"Cooldown de recarga iniciado: \", tiempo_recarga, \" segundos\")

func _on_recarga_completa() -> void:
	puede_disparar = true
	print(\"‚úÖ DISPARO DISPONIBLE NUEVAMENTE\")

func _on_animation_finished() -> void:
	# Detectar si termin√≥ cualquiera de las animaciones de disparo
	var es_animacion_disparo = ($AnimatedSprite2D.animation == \"shot\" or $AnimatedSprite2D.animation == \"armadura_atacar\")
	
	if es_animacion_disparo and esta_disparando:
		print(\"‚ö†Ô∏è Animaci√≥n de disparo termin√≥ - Backup finalizar disparo\")
		if esta_disparando:
			esta_disparando = false

func resetear_disparo() -> void:
	print(\"üîÑ RESETEO DE EMERGENCIA DEL DISPARO\")
	esta_disparando = false
	puede_disparar = true
	if timer_recarga:
		timer_recarga.stop()

# NUEVA FUNCI√ìN: Configurar d√≥nde se fija la c√°mara al caer
func configurar_posicion_fija_camara(posicion: Vector2) -> void:
	posicion_fija_camara = posicion
	print(\"üì∑ NUEVA posici√≥n fija de c√°mara configurada en: \", posicion)

# NUEVA FUNCI√ìN: Obtener posici√≥n actual para configurar punto fijo
func marcar_posicion_actual_como_fija() -> void:
	if has_node(\"Camera2D\"):
		posicion_fija_camara = $Camera2D.global_position
		print(\"üì∑ Posici√≥n actual marcada como punto fijo: \", posicion_fija_camara)
	else:
		posicion_fija_camara = global_position + Vector2(0, -10)
		print(\"üì∑ Usando posici√≥n del jugador como punto fijo: \", posicion_fija_camara)

# NUEVA FUNCI√ìN: Reactivar seguimiento de c√°mara (para testing)
func reactivar_seguimiento_camara() -> void:
	# Buscar la c√°mara en el nivel si fue movida
	var camara_en_nivel = get_parent().get_node_or_null(\"Camera2D\")
	
	if camara_en_nivel:
		# Mover la c√°mara de vuelta al jugador
		get_parent().remove_child(camara_en_nivel)
		add_child(camara_en_nivel)
		camara_en_nivel.position = Vector2(6, -10)  # Posici√≥n relativa original
		camara_en_nivel.position_smoothing_enabled = true
		camara_siguiendo = true
		print(\"üì∑ C√ÅMARA REACTIVADA - Vuelve a seguir al jugador\")
	elif has_node(\"Camera2D\"):
		# La c√°mara sigue siendo hija del jugador
		var camara = $Camera2D
		camara.position_smoothing_enabled = true
		camara_siguiendo = true
		print(\"üì∑ SEGUIMIENTO REACTIVADO\")
	else:
		print(\"‚ö†Ô∏è No se encontr√≥ c√°mara para reactivar\")

# NUEVA FUNCI√ìN: Resetear control de ca√≠da
func resetear_control_caida() -> void:
	print(\"üîÑ RESETEO DEL CONTROL DE CA√çDA\")
	ya_inicio_muerte_caida = false
	camara_siguiendo = true
	reactivar_seguimiento_camara()

# NUEVA FUNCI√ìN: Teletransportar para testing
func teletransportar_a_seguro(posicion: Vector2) -> void:
	global_position = posicion
	velocity = Vector2.ZERO
	resetear_control_caida()
	print(\"üìç Teletransportado a posici√≥n segura: \", posicion)

# NUEVA FUNCI√ìN: Activar/desactivar c√°mara fija en saltos
func alternar_camara_fija_saltos(activar: bool) -> void:
	if activar:
		print(\"üéÆ C√ÅMARA FIJA EN SALTOS: ACTIVADA\")
		print(\"  - La c√°mara se quedar√° fija cuando saltes\")
	else:
		print(\"üéÆ C√ÅMARA FIJA EN SALTOS: DESACTIVADA\") 
		print(\"  - La c√°mara seguir√° al jugador normalmente\")
		# Si est√° desactivado, asegurar que siempre siga
		if has_node(\"Camera2D\"):
			$Camera2D.position_smoothing_enabled = true
			camara_siguiendo = true

# NUEVA FUNCI√ìN: Forzar reactivaci√≥n de c√°mara (para emergencias)
func forzar_reactivacion_camara() -> void:
	print(\"üîß FORZANDO REACTIVACI√ìN DE C√ÅMARA\")
	if has_node(\"Camera2D\"):
		var camara = $Camera2D
		camara.position_smoothing_enabled = true
		camara_siguiendo = true
		print(\"‚úÖ C√°mara forzada a seguir jugador\")

# FUNCI√ìN DE DA√ëO CON AUDIO SEGURO
func bajar_vida(cantidad: int) -> void:
	if esta_muerto:
		return

	if tiene_armadura:
		tiene_armadura = false
		# Audio seguro
		if not esta_sonido_reproduciendose(sonido_da√±o):
			reproducir_sonido_seguro(sonido_da√±o)
		print(\"¬°La armadura se rompi√≥!\")
	else:
		vida -= cantidad
		if vida <= 32:
			vida = 32
			actualizar_barra_vida()
			await morir()
			return
		actualizar_barra_vida()

		# Audio seguro
		if not esta_sonido_reproduciendose(sonido_da√±o):
			reproducir_sonido_seguro(sonido_da√±o)

		$AnimatedSprite2D.play(\"hurt\")
		velocity = Vector2(-50 if $AnimatedSprite2D.flip_h else 50, -10)
		await parpadeo_rojo()
		print(\"¬°Me han golpeado! Vida restante:\", vida)

func parpadeo_rojo() -> void:
	for i in 3:
		$AnimatedSprite2D.modulate = Color(1, 0.2, 0.2)
		await get_tree().create_timer(0.1).timeout
		$AnimatedSprite2D.modulate = Color(1, 1, 1)
		await get_tree().create_timer(0.1).timeout

func subir_vida(cantidad: int) -> void:
	if esta_muerto:
		return

	vida += cantidad
	if vida > vida_maxima:
		vida = vida_maxima
	
	actualizar_barra_vida()
	reproducir_sonido_seguro(sonido_curar_vida)
	print(\"¬°Vida restaurada! Vida actual:\", vida)

func activar_armadura() -> void:
	tiene_armadura = true
	reproducir_sonido_seguro(sonido_armadura)
	print(\"¬°Armadura activada!\")

func actualizar_barra_vida() -> void:
	if barra_vida != null:
		barra_vida.value = vida
		actualizar_color_barra_vida()

func actualizar_color_barra_vida() -> void:
	var porcentaje := float(vida) / vida_maxima

	if porcentaje <= 0.5:
		barra_vida.tint_progress = Color(1, 0, 0)  # Rojo
	elif porcentaje <= 0.64:
		barra_vida.tint_progress = Color(255, 255, 0)  # Amarillo brillante
	else:
		barra_vida.tint_progress = Color(0, 1, 0)  # Verde

func morir() -> void:
	if esta_muerto:
		return
	esta_muerto = true
	
	reproducir_sonido_seguro(sonido_muerte)
	
	# Detener sonidos de forma segura
	detener_sonido_seguro(sonido_pasos)
	
	velocity = Vector2.ZERO
	$AnimatedSprite2D.play(\"dead\")
	await $AnimatedSprite2D.animation_finished
	$AnimatedSprite2D.stop()
	$AnimatedSprite2D.frame = $AnimatedSprite2D.sprite_frames.get_frame_count(\"dead\") - 1
